<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="TestTextBoxInExpander.InterceptAtTextBox"
        Title="Intercept space key events at the TextBox">
    <Expander>
        <Expander.Header>
            <TextBox Classes="InterceptSpaces" Name="txtInsideHeader" KeyDown="txtInsideHeader_KeyDown" KeyUp="txtInsideHeader_KeyUp" />
        </Expander.Header>
        Welcome to Avalonia!
    </Expander>
</Window>
